                    ul.social
                      li
                        a(href='#')
                          i.icon-s-twitter
                      li
                        a(href='#')
                          i.icon-s-facebook
                      li
                        a(href='#')
                          i.icon-s-gplus
                
            .light-wrapper
              .container.inner
                .section-title.text-center
                  h3.link-effect Contactar
                  .col-sm-4
                    br
                  .col-sm-4
                    .date-title
                      form(action='https://formspree.io/#{thing.mail}', method='POST')
                        input(type='text', name='name')
                        input(type='email', name='_replyto')
                        input(type='submit', value='Send')  
            // /.row

            // /.timeline
