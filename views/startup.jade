doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    meta(name='description', content='')
    meta(name='author', content='')
    link(rel='shortcut icon', href='../style/images/favicon.png')
    title= title
    // Bootstrap core CSS
    link(href='../style/css/bootstrap.min.css', rel='stylesheet')
    link(href='../style/css/plugins.css', rel='stylesheet')
    link(href='../style.css', rel='stylesheet')
    link(href='../style/css/color/blue.css', rel='stylesheet')
    link(href='http://fonts.googleapis.com/css?family=Montserrat:400,700', rel='stylesheet', type='text/css')
    link(href='http://fonts.googleapis.com/css?family=Lato:300,400,700,900,300italic,400italic,700italic,900italic', rel='stylesheet', type='text/css')
    link(href='../style/type/icons.css', rel='stylesheet')
    // HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries
    // WARNING: Respond.js doesn't work if you view the page via file://
    //if lt IE 9
      script(src='https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js')
      script(src='https://oss.maxcdn.com/respond/1.4.2/respond.min.js')
  body
    #preloader
      #status
        .spinner
    .body-wrapper
      .navbar.navbar-default.default
        .container
          .navbar-header
            .basic-wrapper
              a.btn.responsive-menu(data-toggle='collapse', data-target='.navbar-collapse')
                i
              .navbar-brand
                a(href='/', style='color:#006ba0')
                  Team TeamStarter
                  // img.logo-light(src='#', srcset='../style/images/logo.png 1x, ../style/images/logo@2x.png 2x', alt='')
                  // img.logo-dark(src='#', srcset='../style/images/logo-dark.png 1x, ../style/images/logo-dark@2x.png 2x', alt='')
              // /.navbar-brand
            // /.basic-wrapper
          // /.navbar-header
          nav.collapse.navbar-collapse
            ul.nav.navbar-nav
              li
                a(href='/') Inicio
              li
                a(href='/startups') Startups
              li
                a(href='/emprendedores') Emprendedores
              li
                a(href='/registro') Registro/Login
            // /.navbar-nav
          // /.navbar-collapse
        // /.container
      // /.navbar
      each thing in data
        .light-wrapper
          .container.inner
            .section-title.text-center
              h3.link-effect=thing.nombre
            .row
              .col-sm-6.timeline-item
                br
                br
                p=thing.resumen
              .col-sm-6.timeline-item.right
                .arrow
                .post-content
                  .row
                    .col-sm-3.col-md-5
                      figure
                        a
                          .text-overlay
                            .info
                              span .
                          img(src='../style/images/art/tb1.jpg', alt='')
                    // /column
                    .col-sm-9.col-md-7
                      h4.post-title
                        p.link-effect.text-center=thing.nombre
                      .meta
                        span.category
                          a=thing.nivel
                        br
                      p=thing.perfiles
                      a.more.link-effect(href='/startup/vivelus') 
                    // /column
                  // /.row
                // /.post-content
              // /column
              .col-sm-12
                .section-title.text-center
                  h4.link-effect Descripcion
                p.text-center=thing.descripcion
              
            // /.row
            .light-wrapper
              .container.inner
                .section-title.text-center
                  h3 Perfiles actuales
                .row.grid-view
                  .col-sm-3.text-center.emprendedor
                    figure
                      a
                        img.img-circle.img-emp(src='../style/images/art/t1.jpg', alt='')
                    a 
                      h4 Backend
                    ul.social
                      li
                        a(href='#')
                          i.icon-s-twitter
                      li
                        a(href='#')
                          i.icon-s-facebook
                      li
                        a(href='#')
                          i.icon-s-gplus
            .light-wrapper
              .container.inner
                .section-title.text-center
                  h3 Están Buscando
                .row.grid-view
                  .col-sm-3.text-center.emprendedor
                    figure
                      a
                        img.img-circle.img-emp(src='../style/images/art/t1.jpg', alt='')
                    a 
                      h4 Frontend
                    ul.social
                      li
                        a(href='#')
                          i.icon-s-twitter
                      li
                        a(href='#')
                          i.icon-s-facebook
                      li
                        a(href='#')
                          i.icon-s-gplus
                  .col-sm-3.text-center.emprendedor
